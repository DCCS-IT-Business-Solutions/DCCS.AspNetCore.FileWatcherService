{
  "Logging": {
    "LogLevel": {
      "Default": "Warning"
    }
  },
  "AllowedHosts": "*",
  
    
      "FileWatcherService": {
        "Watches": [
          {
            "Name": "Text Import",
            "Directory": "C:\\ImportManager\\files",
            "SearchPattern": "*.txt",
            "SearchRegExPattern": ".*",
            "CallbackURL": "http://localhost/MakeTextImport"
          },
          {
            "Name": "Excel Import",
            "Directory": "C:\\ImportManager\\files2",
            "SearchPattern": "*.xlsx",
            "SearchRegExPattern": ".*"
          }
        ],
        "DelayInMS": 500
      }
    }
  
}
